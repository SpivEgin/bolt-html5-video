{
  "name": "cdowdy/bolt-html5-video",
  "description": "A Bolt Extension to Add HTML5 Video Field and Twig Function.",
  "type": "bolt-extension",
  "keywords": [
    "html5 video",
    "Save-Data Header"
  ],
  "require": {
    "bolt/bolt": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Cory Dowdy",
      "email": "cory@corydowdy.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Bolt\\Extension\\cdowdy\\html5video\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bolt\\Extension\\cdowdy\\html5video\\Tests\\": "tests",
      "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
    }
  },
  "extra": {
    "bolt-assets": "web",
    "bolt-class": "Bolt\\Extension\\cdowdy\\html5video\\Html5VideoExtension"
  }
}
